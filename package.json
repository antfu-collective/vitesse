    "markdown-it-shiki": "^0.9.0",
    "pnpm": "^8.4.0",
    "shiki": "^0.14.2",
    "simple-git-hooks": "^2.8.1",
    "taze": "^0.10.1",
    "typescript": "^5.0.4",
    "unocss": "^0.51.12",
